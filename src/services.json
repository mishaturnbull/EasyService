      "start": "/etc/init.d/nessusd start",
      "stop": "/etc/init.d/nessusd stop"
    },
    {
      "name": "Apache 2",
      "computer_name": "apache2",
      "category": "Networking",
      "start": "/etc/init.d/apache2 start",
